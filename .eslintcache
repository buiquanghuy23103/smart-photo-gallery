ios.get(url)\n            .then(res => {\n                const newPictureList = query ? res.data.results : res.data;\n                if (page > FIRST_PAGE_NUMBER) {\n                    setPictureList([...pictureList, ...newPictureList]);\n                } else {\n                    setPictureList([...newPictureList]);\n                }\n                setErrors([]);\n            })\n            .catch(err => {\n                console.log(`errors=${err}`);\n                setErrors(err.response.data.errors);\n            });\n    }\n\n    useEffect(() => {\n        if (query === null) return;\n        resetPageNumber();\n        downloadPictures();\n    }, [query])\n\n    useEffect(() => {\n        downloadPictures();\n    }, [page])\n\n\n\n    return [pictureList, setPictureList, errors, page, setPage] as const;\n}\n","/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/LoadingComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useDebounce.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/HomePageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/LoginPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/GalleryPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/route.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/HeaderComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/config/firebase.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/store/AppContext.tsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":1,"column":8,"nodeType":"80","messageId":"81","endLine":1,"endColumn":13},{"ruleId":"82","severity":1,"message":"83","line":18,"column":8,"nodeType":"84","endLine":18,"endColumn":10,"suggestions":"85"},{"ruleId":"82","severity":1,"message":"86","line":44,"column":8,"nodeType":"84","endLine":44,"endColu