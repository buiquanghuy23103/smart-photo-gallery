witch (route.routeType) {\n\n              case RouteType.auth:\n                return (<AuthRoute\n                  key={ route.path }\n                  path={ route.path }\n                  exact={ route.exact }\n                >\n                  <route.component />\n                </AuthRoute>\n                );\n\n              case RouteType.guest:\n                return (<GuestRoute\n                  key={ route.path }\n                  path={ route.path }\n                  exact={ route.exact }\n                >\n\n                  <route.component />\n                </GuestRoute>);\n\n              case RouteType.default:\n                return (<AnimatedRoute\n                  key={ route.path }\n                  path={ route.path }\n                  exact={ route.exact }\n                >\n\n                  <route.component />\n\n                </AnimatedRoute>);\n              default:\n                return (<Route\n                  key={ route.path }\n                  path={ route.path }\n                  exact={ route.exact }\n                  component={ route.component }\n                >\n                  <motion.div\n                    initial={ { x: 200 } }\n                    animate={ {\n                      x: 0,\n                      transition: { duration: 1 }\n                    } }\n                  >\n                    <route.component />\n                  </motion.div>\n                </Route>);\n            }\n          }) }\n\n          <Route path=\"*\" component={ NotFoundPageComponent } />\n        </Switch>\n      </AppContext.Provider>\n    </BrowserRouter>\n  )\n\n}\n\nexport default App;\n","/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/PictureComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useScroll.tsx",["96","97"],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useFetchPicture.tsx",["98","99"],"import axios from 'axios';\nimport { useEffect, useState } from 'react';\nimport { Picture } from '../../types/PictureResponseTypes';\n\nconst BASE_URL = process.env.REACT_APP_UNSPLASH_URL;\nconst PUBLIC_KEY = process.env.REACT_APP_UNSPLASH_PUBLIC_KEY;\nconst FIRST_PAGE_NUMBER = 1;\n\nexport default function useFetchPicture(query: string | null) {\n    const [pictureList, setPictureList] = useState([] as Picture[]);\n    const [errors, setErrors] = useState([] as string[]);\n    const [page, setPage] = useState(FIRST_PAGE_NUMBER);\n\n    function resetPageNumber() {\n        setPage(FIRST_PAGE_NUMBER);\n    }\n\n    function downloadPictures() {\n        const api = !!query ? `/search/photos?query=${query}&` : \"/photos?\"\n        const url = `${BASE_URL}${api}client_id=${PUBLIC_KEY}&query=${query}&page=${page}`;\n\n        console.log(url);\n\n        axios.get(url)\n            .then(res => {\n                const newPictureList = query ? res.data.results : res.data;\n                if (page > FIRST_PAGE_NUMBER) {\n                    setPictureList([...pictureList, ...newPictureList]);\n                } else {\n                    setPictureList([...newPictureList]);\n                }\n                setErrors([]);\n            })\n            .catch(err => {\n                console.log(`errors=${err}`);\n                setErrors(err.response.data.errors);\n            });\n    }\n\n    useEffect(() => {\n        if (query === null) return;\n        resetPageNumber();\n        downloadPictures();\n    }, [query])\n\n    useEffect(() => {\n        downloadPictures();\n    }, [page])\n\n\n\n    return [pictureList, setPictureList, errors, page, setPage] as const;\n}\n","/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/LoadingComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useDebounce.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/HomePageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/LoginP