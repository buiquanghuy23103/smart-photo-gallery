;\n    }\n\n    function downloadPictures() {\n        const api = !!query ? `/search/photos?query=${query}&` : \"/photos?\"\n        const url = `${BASE_URL}${api}client_id=${PUBLIC_KEY}&query=${query}&page=${page}`;\n\n        console.log(url);\n\n        axios.get(url)\n            .then(res => {\n                const newPictureList = query ? res.data.results : res.data;\n                if (page > FIRST_PAGE_NUMBER) {\n                    setPictureList([...pictureList, ...newPictureList]);\n                } else {\n                    setPictureList([...newPictureList]);\n                }\n                setErrors([]);\n            })\n            .catch(err => {\n                console.log(`errors=${err}`);\n                setErrors(err.response.data.errors);\n            });\n    }\n\n    useEffect(() => {\n        if (query === null) return;\n        resetPageNumber();\n        downloadPictures();\n    }, [query])\n\n    useEffect(() => {\n        downloadPictures();\n    }, [page])\n\n\n\n    return [pictureList, setPictureList, errors, page, setPage] as const;\n}\n","/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/LoadingComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useDebounce.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/HomePageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/LoginPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/GalleryPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/HeaderComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/config/firebase.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/store/AppContext.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/routes/routes.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/routes/AuthRoute.tsx",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replaced