se {\n                    setPictureList([...newPictureList]);\n                }\n                setErrors([]);\n            })\n            .catch(err => {\n                console.log(`errors=${err}`);\n                setErrors(err.response.data.errors);\n            });\n    }\n\n    useEffect(() => {\n        if (query === null) return;\n        resetPageNumber();\n        downloadPictures();\n    }, [query])\n\n    useEffect(() => {\n        downloadPictures();\n    }, [page])\n\n\n\n    return [pictureList, setPictureList, errors, page, setPage] as const;\n}\n","/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/LoadingComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/utils/hooks/useDebounce.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/HomePageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/LoginPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/GalleryPageComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/pages/route.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/components/HeaderComponent.tsx",[],"/Volumes/MacintoshHD2/huybui/tutorial-10-hours/src/config/firebase.tsx",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":1,"column":8,"nodeType":"76","messageId":"77","endLine":1,"endColumn":13},{"ruleId":"78","severity":1,"message":"79","line":18,"column":8,"nodeType":"80","endLine":18,"endColumn":10,"suggestions":"81"},{"ruleId":"78","severity":1,"message":"82","line":44,"column":8,"nodeType":"80","endLine":44,"endColumn":15,"suggestions":"83"},{"ruleId":"78","severity":1,"message":"82","line":48,"column":8,"nodeType":"80","endLine":48,"endColumn":14,"suggestions":"84"},"no-native-reassign",["85"],"no-negated-in-lhs",["86"],"@typescript-eslint/no-unused-vars","'React' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","